{
  "version": 2,
  "buildCommand": "cd src/frontend && npm install && npm run build",
  "devCommand": "cd src/frontend && npm run dev",
  "installCommand": "cd src/frontend && npm install",
  "framework": "nextjs",
  "outputDirectory": "src/frontend/.next",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://mind-scroll-production.up.railway.app/api/:path*"
    }
  ]
}

